{
  "name": "manaobot",
  "version": "1.0.8",
  "description": "Collection of utilities for Twitch chat and API",
  "main": "index.ts",
  "scripts": {
    "start": "bun build:css && bun index.ts",
    "start:watch": "bun --watch index.ts",
    "build": "tsc -p . && cp .env dist",
    "build:css": "bun x @tailwindcss/cli -i ./server/public/css/tailwind.css -o ./server/public/css/dist/tailwind.css ",
    "build:watch": "tsc -p . --watch",
    "dev": "conc --kill-others \"bun run build:css\" \"bun run start:watch\"",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "prettier": "prettier --write ./**/*.{js,ts,html,css}",
    "prettier:check": "prettier --check ./**/*.{js,ts,html,css}"
  },
  "keywords": [
    "twitch-bot"
  ],
  "authors": [
    "Tinnaphat Somsang <tinvv@outlook.co.th>"
  ],
  "license": "MPL-2.0",
  "type": "module",
  "dependencies": {
    "@distube/ytdl-core": "4.16.0",
    "@elysiajs/static": "^1.2.0",
    "@inquirer/prompts": "^7.5.0",
    "@twurple/api": "^7.2.1",
    "@twurple/auth": "^7.2.1",
    "@twurple/chat": "^7.2.1",
    "@twurple/eventsub-ws": "^7.2.1",
    "@types/tough-cookie": "^4.0.5",
    "chalk": "^5.4.1",
    "cors": "^2.8.5",
    "elysia": "^0.8.17",
    "express": "^4.18.2",
    "socket.io": "^4.7.4",
    "tailwindcss": "^4.1.5",
    "youtube-sr": "^4.3.10"
  },
  "devDependencies": {
    "@types/bun": "^1.2.10",
    "@types/express": "^4.17.21",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "concurrently": "^9.1.2",
    "daisyui": "^5.0.35",
    "eslint": "^8.0.1",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-import-resolver-typescript-bun": "^0.0.104",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-readable-tailwind": "^2.1.1",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "typescript": "^5.3.3"
  }
}
